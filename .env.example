# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Email Configuration (Auto-sending)
SMTP_HOST=mail.abctravels.site
SMTP_PORT=587
SMTP_USER=your_email@abctravels.site
SMTP_PASSWORD=your_email_password
SMTP_FROM=noreply@abctravels.site
# Note: For Service Role Key above, replace with actual key from Supabase Settings > API

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=JKLG Travel
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Analytics & Monitoring
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=

# Security
VITE_CSRF_TOKEN_HEADER=X-CSRF-Token
VITE_MAX_LOGIN_ATTEMPTS=5
VITE_SESSION_TIMEOUT=3600000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true


